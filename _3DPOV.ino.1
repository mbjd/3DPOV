/**
 * Changing values. Volatile because they are handled within an Interrupt
 */

// Tracks the current angle in units of 3.6. Incremented at the end of sendData().
volatile unsigned int currentPixel = 0; 

// Accounts for the double-helix arrangement of the PCBs (consult the reddit links in README.md
// for images)
volatile unsigned int offsetPixel = 0;

// The PCB that is currently being sent data. Top => 0, bottom => 9. Only used in sendData().
volatile unsigned int currentBoard = 0;

// An elapsedMicros variable automatically increments each s. Implementing the same behaviour
// using micros() would require additional calculations, since an elapsedMicros can be reset.
// It is used for calculating the period's moving average at each full rotation (hall sensor    
// interrupt calls timerUpdate()) and then reset to 0.
elapsedMicros sinceMagnet;

// Used to store the period's moving average. See timerUpdate().
// Initialised with a high value so it's not rounded down to 0 right after each new measurement.
volatile unsigned long periodAverage = 0x4000;


/**
 * Fixed values
 */

// A short pulse on this pin moves the received data from the input shift registers to the output
// latches of the LED drivers
const unsigned int latchPin = 5;

// When this pin is at VCC, the outputs drivers of the TLC5927 chips are in a high-impedance state,
// turning the outputs off. When it is at GND, they are controlled by the data in the output latch.
const unsigned int outputEnable=4;

// The hall sensor for rotational synchronisation is connected to this pin. 0V -> Magnet is close.
const unsigned int hallPin = 20;

// SPISettings object used to begin an SPI transaction with. First parameter is the bus clock speed
// in Hz, the second one is the bit order and the third one is the clock polarity and phase.
SPISettings mySettings(48000000, LSBFIRST, SPI_MODE0);

// Timer object used to call sendData() 100 times per rotation. At each timerUpdate() it is stopped
// and started again, with its period adjusted for the current rotational period.
IntervalTimer outputTimer;

/**
 * - Sets the pin modes, starts SPI.h and attaches the interrupt
 * - Is called when the program starts
 */

void setup() 
{
  // Initialise pins and SPI

  // Hall pin needs to be pulled up; it only pulls down if there is a magnetic field.
  pinMode(hallPin, INPUT_PULLUP);
  pinMode(outputEnable, OUTPUT);
  pinMode(latchPin, OUTPUT);
  SPI.begin();

  // As soon as the magnet is detected, this Interrupt calls timerUpdate().
  attachInterrupt(hallPin, timerUpdate, FALLING);
} // End of setup()

/**
 * - Empty loop(), since the entire program flow is triggered by the interrupt calling timerUpdate().
 */

void loop() {}

/**
 * - Measures the rotational period and refreshes outputTimer accordingly.
 * - Is called at each falling edge of the hall sensor, i.e. when a full rotation is finished.
 */

void timerUpdate() 
{
  // Measure the time and smooth it out using an exponential moving average
  // Division by 2^n is a simple right shift by n
  periodAverage = (sinceMagnet + 15*periodAverage)/16;

  // Reset everything
  sinceMagnet = 0;
  currentPixel = 0;
  outputTimer.end();

  // Start timer again
  outputTimer.begin(sendData, periodAverage/100);
} // End of timerUpdate()

/**
 * - Sends new data from the previously defined byteArray based on currentPixel's value.
 * - Is called by outputTimer 100 times per full rotation.
 */

void sendData() 
{
  SPI.beginTransaction(mySettings);

  // Cycles through five pairs of PCBs.
  for(int i=0; i<5; i++) 
  {
    // Move to the first PCB of the next pair.
    currentBoard = 2*i;

    // +8*i because each pair of PCBs is offset to the one above it by 8/100 of a whole circle.
    // +70 so that the displayed image is not rotated
    offsetPixel = (currentPixel + 8*i + 70)%100;

    // Cycle through the array's least significant index. In reverse so that the displayed data
    // are the same visually as they are in byteArray, simplifying the the tedious image creation.
    for(int j=5; j>=0; j--)
    {
      SPI.transfer(byteArray[offsetPixel][currentBoard][j]);
    }
    
    // Move down to the other PCB of the current pair.
    currentBoard++;

    // The current PCB is face to face to the previous one, so offsetPixel (the temporary angle) is
    // incremented by half a circle. %100 ensures that no inexistent array positions are used.
    offsetPixel = (offsetPixel+50)%100;

    for(int j=5; j>=0; j--)
    {
      SPI.transfer(byteArray[offsetPixel][currentBoard][j]);
    }
  } // End of outer for loop.

  // Latch the data and finish the transaction.
  digitalWrite(latchPin, HIGH);
  delayMicroseconds(1);
  digitalWrite(latchPin, LOW);

  SPI.endTransaction();
  
  // Increment the angle so that it is correct at the next call of sendData()
  currentPixel++;
} // End of sendData()
